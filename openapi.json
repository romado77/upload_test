{
    "openapi": "3.0.1",
    "info": {
      "title": "Upload test",
      "description": "Upload test",
      "contact": {
        "email": "drv@cisco.com"
      },
      "version": "1.0"
    },
    "servers": [
      {
        "url": "/api/v1"
      }
    ],
    "paths": {
      "/": {
        "post": {
          "summary": "Upload path",
          "description": "Accepting upload request",
          "operationId": "root.post",
          "requestBody": {
            "content": {
              "multipart/form-data": {
                "schema": {
                  "properties": {
                    "devicename": {
                      "type": "string",
                      "example": "SEP5006AB805584"
                    },
                    "serialno": {
                      "type": "string",
                      "example": "PUC19420E1I"
                    },
                    "username": {
                      "type": "string"
                    },
                    "prt_file": {
                      "type": "string",
                      "example": "prt-20200731-142448-5006AB805584.tar.gz",
                      "format": "binary"
                    },
                    "zipFileName": {
                      "type": "string",
                      "example": "Jabber-Win-12.7.0.288632-20200728_000556-Windows_10_Enterprise.zip",
                      "format": "binary"
                    }
                  }
                },
                "encoding": {
                  "prt_file":{
                    "contentType": "application/gzip, application/zip"
                  },
                  "zipFileName":{
                    "contentType": "application/gzip, application/zip"
                  }
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "File uploaded"
            },
            "500": {
              "description": "Upload error"
            }
          }
        }
      }
    }
  }
